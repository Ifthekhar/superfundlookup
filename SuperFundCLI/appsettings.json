{
    "Configuration": {
      "Title": "SuperFund CLI Application"
    },
   
    "Logging": {
      "IncludeScopes": false,
   
      "Debug": {
        "LogLevel": {
          "Default": "Trace"
        }
      },
      "Console": {
        "LogLevel": {
          "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
          "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
          "Microsoft.AspNetCore.Mvc.Razor": "Error",
          "Default": "Trace"
        }
      },
      "LogLevel": {
        "Default": "Trace"
      }
    },
   
    "Serilog": {
      "LogFile": "SuperFundCLI.log",
      "MinimumLevel": "Verbose"
    }
  }